    function updateButtonState() {
      // Determinar qu paso est activo
      const isPaso2Active = !paso2.classList.contains("ocultar");
      const isVentaActive = isPaso2Active && valorCaso === "venta";
      const isPostventaActive = isPaso2Active && valorCaso === "postventa";

      let inputs = [];

      if (isVentaActive) {
        // Para venta: todos los inputs necesarios
        inputs = Array.from(
          document.querySelectorAll(".paso2 .venta input:not([disabled]):not(.opcional), .paso2 .venta select:not([disabled]):not(.opcional)")
        ).filter(input => input.id !== "promociones");
      } else if (isPostventaActive) {
        // Para postventa: todos los inputs necesarios
        inputs = Array.from(
          document.querySelectorAll(".paso2 .postventa input:not([disabled]):not(.opcional), .paso2 .postventa select:not([disabled]):not(.opcional)")
        ).filter(input => input.id !== "promociones");
      } else {
        // Si no est en el paso 2, no hacer nada
        submitButton.classList.remove("active");
        submitButton.setAttribute("disabled", true);
        return;
      }

      // Incluir tambin el checkbox de tratamiento de datos
      const tratamientoCheckbox = document.querySelector("input#tratamiento");
      if (tratamientoCheckbox && !inputs.includes(tratamientoCheckbox)) {
        inputs.push(tratamientoCheckbox);
      }

      // Verificar si todos los inputs tienen la clase "valid"
      const allValid = inputs.every((input) => {
        // Para checkboxes, verificar si estn checked
        if (input.type === 'checkbox') {
          return input.checked;
        }
        // Para selects, verificar que no sea la opcin por defecto
        if (input.tagName === 'SELECT') {
          return input.value !== input.options[0].value;
        }
        // Para otros inputs, verificar si tienen la clase valid
        return input.classList.contains("valid");
      });

      // Si todos los inputs son vlidos, habilitar el botn de enviar
      if (allValid) {
        submitButton.classList.add("active");
        submitButton.removeAttribute("disabled");
      } else {
        submitButton.classList.remove("active");
        submitButton.setAttribute("disabled", true);
      }
    }

    // Evento especfico para el checkbox con ID tratamiento
    const tratamientoCheckbox = document.querySelector("input#tratamiento");
    if (tratamientoCheckbox) {
      tratamientoCheckbox.addEventListener("change", function () {
        // Si est seleccionado, agregar la clase valid; de lo contrario, eliminarla
        if (tratamientoCheckbox.checked) {
          tratamientoCheckbox.classList.add("valid");
        } else {
          tratamientoCheckbox.classList.remove("valid");
        }
        updateButtonState(); // Actualizar el estado despus del cambio
      });
    }

    // Observar cambios en los inputs para refrescar la lista
    form.addEventListener("input", updateButtonState);
