        label.parentElement.classList.add("active");
      });
    });

    const inputsText = document.querySelectorAll(
      'input[type="text"]:not(.opcional)'
    );

    const paso1 = document.querySelector(".paso1");
    const paso2 = document.querySelector(".paso2");

    inputsText.forEach((input) => {
      input.addEventListener("input", () => {
        const value = input.value;

        // Validacin: contiene texto sin nmeros y ms de 1 carcter
        if (/^[a-zA-Z\s]+$/.test(value) && value.length > 1) {
          input.classList.add("valid");
          input.classList.remove("invalid");
        }
        // Si contiene nmeros o est vaco
        else {
          input.classList.add("invalid");
          input.classList.remove("valid");
        }
      });
    });

    const nombreInput = document.querySelector(
      ".formBC__main-input.inputgeneral"
    );

    nombreInput.addEventListener("input", () => {
      const value = nombreInput.value;

      // Validacin: solo letras, espacios, guiones, apstrofes y tildes
      if (/^[a-zA-Z\s'-]+$/.test(value)) {
        nombreInput.classList.add("valid");
        nombreInput.classList.remove("invalid");
      } else {
        nombreInput.classList.add("invalid");
        nombreInput.classList.remove("valid");
      }

      // Elimina cualquier nmero o carcter no permitido en tiempo real
      nombreInput.value = value.replace(/[^a-zA-Z\s'-]/g, "");
