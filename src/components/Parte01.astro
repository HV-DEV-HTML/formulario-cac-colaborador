    if (buttonPaquetesTV) {
      buttonPaquetesTV.forEach(element => {
        element.addEventListener("click", function () {
          const label = document.querySelector(
            '.portlet[data-portlet="bannerform"] .section__align .content .formBC__main .radio label[for="venta"]'
          );
          if (label) {
            label.click();
            const target = document.querySelector('section.contentFormulario');
            if (target) {
              const offset = 100; // píxeles más arriba
              const elementPosition = target.getBoundingClientRect().top + window.pageYOffset;
              const offsetPosition = elementPosition - offset;

              window.scrollTo({
