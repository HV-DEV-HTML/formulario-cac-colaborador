    const sliderNoPresenciales = document.querySelector("section.tramites .sliderNoPresenciales");

    // función para actualizar según tab activo
    function actualizarSliders(tabActivo) {
      const categoria = tabActivo.getAttribute("category");

      // quitar active de todos
      tabs.forEach(t => t.classList.remove("active"));
      sliderPresenciales.classList.remove("active");
      sliderNoPresenciales.classList.remove("active");

      // activar el tab clicado
      tabActivo.classList.add("active");

      // activar el slider según la categoría
      if (categoria === "Presenciales") {
        sliderPresenciales.classList.add("active");
      } else if (categoria === "No presenciales") {
        sliderNoPresenciales.classList.add("active");
      }
    }

    // inicializar con el tab que ya tiene "active"
    const tabInicial = document.querySelector("section.tramites .tabsTramites .tab.active");
    if (tabInicial) {
      actualizarSliders(tabInicial);
    }

    // evento click en cada tab
    tabs.forEach(tab => {
      tab.addEventListener("click", function () {
        actualizarSliders(this);
      });
    });


    const tabs2 = document.querySelectorAll(".tabsInfo .tab");
    const sections = document.querySelectorAll("section[category]");

    tabs2.forEach(tab => {
      tab.addEventListener("click", function () {
        const category = this.getAttribute("category");

        // quitar active de todos los tabs del grupo .tabsInfo
        tabs2.forEach(t => t.classList.remove("active"));
        this.classList.add("active");

        // ocultar todas las secciones
        sections.forEach(sec => {
          sec.classList.remove("active");
          if (sec.getAttribute("category") === category) {
            sec.classList.add("active");
          }
        });
      });
    });

